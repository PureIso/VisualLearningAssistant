
<Page
   Name="SaveMenuPage"
   x:Class="VLAShortestPath.SaveMenu"
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   d:DesignHeight="74"
   d:DesignWidth="178"
   Title="SaveMenu"
   mc:Ignorable="d">
   <Grid Background="Beige">
      <TextBlock
         Name="textBlock1"
         Width="71"
         Height="23"
         HorizontalAlignment="Left"
         Margin="12,12,0,0"
         VerticalAlignment="Top"
         Text="Save Name:"/>
      <Button
         Name="button1"
         Width="75"
         Height="23"
         HorizontalAlignment="Left"
         Margin="90,39,0,0"
         VerticalAlignment="Top"
         Content="Save"/>
      <TextBox
         Name="textBox1"
         Width="75"
         Height="23"
         HorizontalAlignment="Left"
         Margin="90,12,0,0"
         VerticalAlignment="Top"/>
   </Grid>
</Page>